<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <sg_integration>
            <general>
                <enabled>1</enabled>
                <export_categories_enabled>1</export_categories_enabled>
                <export_orders_enabled>1</export_orders_enabled>
                <debug_mode>0</debug_mode>
            </general>
            <middleware>
                <host></host>
                <access_token></access_token>
            </middleware>
        </sg_integration>
        <carriers>
            <gls>
                <active>0</active>
                <tite>GLS</tite>
                <name>GLS</name>
                <is_online>1</is_online>
                <model>ScaliaGroup\Integration\Model\Carrier\Gls</model>
            </gls>
            <tntit>
                <active>0</active>
                <tite>TNT</tite>
                <name>TNT</name>
                <is_online>1</is_online>
                <model>ScaliaGroup\Integration\Model\Carrier\TNT</model>
            </tntit>
        </carriers>
    </default>
</config>
